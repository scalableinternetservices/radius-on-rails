<p class="navbar-text float-right">
<%= link_to 'Show profile', profile_path, :class => 'navbar-link' %>
</p>

<h1>Hi <%= current_user.name %>! These people are nearby:</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Bio</th>
      <th>Lat</th>
      <th>Lng</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.bio %></td>
        <td><%= user.lat %></td>
        <td><%= user.lng %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
